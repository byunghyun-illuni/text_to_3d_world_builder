# Text-to-3D World Builder 환경 설정

# Gemini API 키 (https://aistudio.google.com/apikey)
GEMINI_API_KEY=your-api-key-here

# ModelScope 모델 경로 (gated 모델 우회용)
DINOV3_MODEL_PATH=~/.cache/modelscope/facebook/dinov3-vitl16-pretrain-lvd1689m
RMBG_MODEL_PATH=~/.cache/modelscope/briaai/RMBG-2.0

# CUDA 설정
CUDA_VISIBLE_DEVICES=0
CUDA_HOME=/usr/local/cuda-12.5

# PyTorch 메모리 최적화
PYTORCH_CUDA_ALLOC_CONF=expandable_segments:True

# 서버 설정
HOST=0.0.0.0
PORT=8000
